[merge]
    tool = vimdiff
[core]
    editor = vim
    excludesfile=~/.gitignore
    #autocrlf = input
[color]
    ui = true
    status = auto
    diff = auto
    branch = auto
    grep = auto
    interactive = auto
[alias]
    co = checkout
    st = status
    ci = commit
    br = branch
    gl = log --graph --decorate
    ga = log --graph --decorate --all
    logg = log --stat --pretty=format:'%Cblue%h %Cgreen%ar %Cred%an %Creset%s %Cred%d'
    loggr = log --graph --date-order --pretty=format:'%Cblue%h %Cgreen%ci %Cred%an %Cblue%m %Creset%s %Cred%d'
